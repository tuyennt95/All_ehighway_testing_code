<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="requestSourcing">
    <test name="seller request sourcing">
        <classes>
            <class name="sellerPortal.test.requestSourcingSeller">
                <methods>
                    <include name="requestSourcing2"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="fulfill verify request Sourcing">
        <classes>
            <class name="fulfillPortal.test.requestSourcingFulfill">
                <methods>
                    <include name="respondRejectRequest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="seller verify respond fulfill">
        <classes>
            <class name="sellerPortal.test.requestSourcingSeller">
                <methods>
                    <include name="verifyRespondRejectRequestFromFulfill"/>
                </methods>
            </class>
        </classes>
    </test>
    <!-- Test -->
</suite> <!-- Suite -->
